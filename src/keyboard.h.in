#ifndef KEYBOARD_h_
#define KEYBOARD_h_

#include <stdbool.h>

struct Key {
  const float x;
  const float y;
  const float rot;
  const float width;
  const float height;
  const float width2;
  const float height2;
  bool pressed;
} Key;

struct Key keyboard[] = {
{%- for item in key_data %}
  { {{ item }} },
{%- endfor %}
};

const int keycode_keyboard_lookup[256] = {
{%- for value in lookup %}
  {{ value }},
{%- endfor %}
};

#endif /* KEYBOARD_h_ */
